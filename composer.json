{
  "name": "ttek/tkbase",
  "type": "project",
  "description": "A Tk base MVC framework project",
  "keywords": [
    "tropotek"
  ],
  "time": "2021-06-12",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Michael Mifsud",
      "homepage": "http://www.tropotek.com/"
    }
  ],
  "autoload": {
    "psr-0": {
      "": "src/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Tk\\Composer\\SetupEvent::postInstall"
    ],
    "post-update-cmd": [
      "Tk\\Composer\\SetupEvent::postUpdate"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.2.x-dev"
    }
  },
  "require": {
    "php": "^7.3||^8.0",
    "ttek/tk-base": "~3.2.0",
    "ttek/theme-cube": "~1.2.0",
    "ttek-plg/mailog": "~1.0.0",
    "ttek-plg/events": "~1.0.0",
    "ttek-plg/subscribers": "~1.0.0",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "require-dev": {
  },
  "config": {
    "allow-plugins": {
      "ttek/tk-installers": true
    }
  }
}
